{
  "compilerOptions": {
    "typeRoots": [
      "src/@types", "node_modules/@types"],
    "target": "es2016",
    "outDir": "dist",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
